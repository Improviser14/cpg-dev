  <!-- load particle.js -->
  <script>
    window.onload = function () {
      Particles.init({
        selector: '.background',
        connectParticles: true,
        color: "#00BFFF",
        responsive: [{
          breakpoint: 768,
          options: {
            maxParticles: 80,
            connectParticles: true,
            speed: 1.5
          }
        },
        {
          breakpoint: 660,
          options: {
            maxParticles: 40,
            connectParticles: true,
            speed: 1.5
          }
        }
        ]
      });
    };
  </script>

